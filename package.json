{
  "name": "nuxt-token-authentication",
  "type": "module",
  "version": "1.1.0",
  "description": "Token Authentication for Nuxt Server APIs",
  "license": "MIT",
  "repository": "rrd108/nuxt-token-authentication",
  "keywords": [
    "nuxt",
    "nuxt-module",
    "authentication"
  ],
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "yarn lint && yarn test && yarn prepack && changelogen --release && yarn publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@antfu/eslint-config": "^3.8.0",
    "@nuxt/kit": "^3.14.0",
    "bcrypt": "^5.1.1",
    "defu": "^6.1.4"
  },
  "devDependencies": {
    "@nuxt/devtools": "^1.3.7",
    "@nuxt/eslint-config": "^0.3.13",
    "@nuxt/module-builder": "^0.8.3",
    "@nuxt/schema": "^3.14.0",
    "@nuxt/test-utils": "^3.11.0",
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^20.14.9",
    "better-sqlite3": "^11.1.1",
    "changelogen": "^0.5.5",
    "eslint": "^9.6.0",
    "nuxt": "^3.14.0",
    "tsx": "^4.7.1",
    "typescript": "^5.6.3",
    "vitest": "^1.6.0",
    "vitest-mock-extended": "^1.3.1",
    "vue-tsc": "^2.0.24"
  }
}
